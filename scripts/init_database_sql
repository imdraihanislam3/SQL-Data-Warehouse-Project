*/
Script Purpose : This Script creates a new database named "datawarehouse". The Script sets up 3 schemas 
whithin the database :'bronze', 'silver' and 'gold'.
*/


-- Create Datebase "datawarehouse"

USE master;

CREATE DATABASE datawarehouse;

-- Use Created Warehouse
USE datawarehouse;

-- Create Schemas

CREATE SCHEMA bronze;
GO
CREATE SCHEMA Silver;
GO
CREATE SCHEMA gold;
GO
